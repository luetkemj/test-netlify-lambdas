{
  "name": "netlify-lambdas",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:app": "./node_modules/.bin/babel src/app -d dist",
    "build:functions": "./node_modules/.bin/babel src/functions -d functions",
    "build": "npm run build:app && npm run build:functions",
    "clean": "./node_modules/.bin/rimraf lib",
    "lambdas:build": "rimraf ./lambdas && cp -r src/functions ./lambdas && netlify-lambda build ./lambdas",
    "lambdas:serve": "netlify-lambda serve ./lambdas",
    "lambdas": "npm run lambdas:build && npm run lambdas:serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luetkemj/test-netlify-lambdas.git"
  },
