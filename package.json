  },
  "homepage": "https://github.com/luetkemj/test-netlify-lambdas#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "netlify-lambda": "^0.4.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "jsonwebtoken": "^8.2.1"
  }
}
